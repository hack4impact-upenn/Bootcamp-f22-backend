{
  "name": "boilerplate-s2022",
  "version": "1.0.0",
  "repository": "https://github.com/hack4impact-upenn/boilerplate-s2022.git",
  "license": "MIT",
  "private": true,
  "workspaces": ["client", "server"],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "setup": "yarn install",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "server": "cd server && yarn start",
    "postinstall": "echo \"Package install attempted in global directory\"",
    "client": "cd client && yarn start"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@testing-library/dom": ">=7.21.4",
    "autoprefixer": "^10.0.2",
    "postcss": "^8.1.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.1",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "typescript": "^4.5.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "prettier": "^2.5.1"
  }
}
